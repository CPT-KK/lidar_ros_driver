<launch>
  <param name="use_sim_time" type="bool" value= "true" />
  <node pkg="tf" type="static_transform_publisher" name="flu_to_map" args="0 0 0 0 0 0 1 USV_FLU map  10" /> 
  <rosparam file="$(find lidar_usv_det)/config/lidar_usv_det.yaml" command="load" />
  <node pkg="lidar_usv_det" type="lidar_usv_det" name="lidar_usv_det" output="screen">
    <param name="usv_length" type="double" value="3.6" />
    <param name="usv_width" type="double" value="1.5" />
    <param name="usv_height" type="double" value="5.0" />
    <param name="usv_downsample_factor" type="double" value="0.4" />
    <param name="usv_intensity" type="int" value="10" />
    
  </node>

  
  <arg name="rviz" default="true" />
  <group if="$(arg rviz)">
        <node launch-prefix="nice" pkg="rviz" type="rviz" name="rviz" args="-d $(find lidar_usv_det)/config/rviz.rviz" />
    </group>
</launch>